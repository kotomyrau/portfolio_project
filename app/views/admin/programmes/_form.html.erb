<%= form_with model: [:admin, programme], data: { controller: "" , turbo: false } do |f| %>
  <%#= f.error_notification %>
  <%#= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="field">
    <%= f.label :name, "Name", class: "field__label t-label" %>
    <%= f.text_field :name, autofocus: true, required: true, min: 2, max: 100, class: "field__input" %>
  </div>
  <div class="form__actions">
    <%= f.submit "Add Skill", class: "btn btn--primary" %>
  </div>
<% end %>
